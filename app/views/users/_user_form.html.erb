<%= form_for :user do |f| %>
 <%= f.label :name %><br>
 <%= f.text_field :name %>
 <br>
 <%= f.label :passport_number %><br>
 <%= f.number_field :passport_number %>
 <br>
 <%= f.label :username %><br>
 <%= f.text_field :username %>
 <br>
 <%= f.label :password %><br>
 <%= f.text_field :password %>
 <br>
 <%= f.submit %>
<%end %>
