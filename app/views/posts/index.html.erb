<h1> All Posts </h1>

<% @posts.each do |post| %>
<div class="card" style="width: 18rem;">
    <div class="card-body">
        <h5 class="card-title"><%= link_to post.title, post %></h5>
        <p class="card-text"><%= post.likes.count%>&nbsp<%=(post.likes.count) ==1 ? 'Like': 'Likes'%></p>
    </div>
</div>
<% end %>